{
    "name": "markdown",
    "version": "3.3.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Libraries to process markdown documents",
    "category": [ "Documentation", "Web" ],
    "dependencies": {
        "base" : ">= 3.0.0, < 4.0.0",
        "html2": ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "Text.Markdown" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "executable": {
        "name": "md2pdf",
        "main": "ProcessMarkdown"
    },
    "documentation": {
        "src-dir": "docs",
        "main": "markdown-syntax.md"
    },
    "source": {
        "git": "https://github.com/curry-packages/markdown.git",
        "tag": "$version"
    }
}
